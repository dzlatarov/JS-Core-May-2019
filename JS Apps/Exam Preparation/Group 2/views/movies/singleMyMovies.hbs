<div class="movie">
    <h2>Title: {{title}}</h2>
    <img
        src={{imageUrl}}>
    <p>Available Tickets: {{tickets}}</p>
    <div class="btn-group">
        <button><a href="#/editMovie/{{_id}}">Edit</a></button>
        <button><a href="#/deleteMovie/{{_id}}">Delete</a></button>
        <button><a href="#/buyTickets/{{_id}}">Buy Ticket</a></button>
        <button><a href="#/movieDetails/{{_id}}">Details</a></button>
    </div>
</div>